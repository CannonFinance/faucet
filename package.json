  "scripts": {
    "hash": "echo \"{\\\"HASH\\\": \\\"`git rev-parse HEAD`\\\"}\" > public/commitHash.json",
    "start": ". ./.env || true; npm run hash; node scripts/start.js",
    "build": ". ./.env || true; npm run hash; node scripts/build.js --max-old-space-size=4096",
    "test": "npm run hash; node scripts/test.js --env=jsdom",
    "predeploy": "npm run hash; npm run build",
    "deploy": "npm run hash; gh-pages -d build"
  },
